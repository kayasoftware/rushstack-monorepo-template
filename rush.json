{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.61.2",
  "pnpmVersion": "6.28.0",
  "pnpmOptions": {
    "pnpmStore": "local",
    "preventManualShrinkwrapChanges": true,
    "resolutionStrategy": "fewer-dependencies",
    "strictPeerDependencies": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=14.15.0 <15.0.0",
  "suppressNodeLtsWarning": true,
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 3,
  "allowMostlyStandardPackageNames": false,
  "gitPolicy": {
    "versionBumpCommitMessage": "chore: ðŸ¤– applying package updates"
  },
  "repository": {
    "url": "https://github.com/{accountName}/{repoName}",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "telemetryEnabled": false,
  "projects": [
    {
      "packageName": "@kayasoftware/sample-app",
      "projectFolder": "apps/sample-app",
      "shouldPublish": false
    },
    {
      "packageName": "@kayasoftware/heft-node-rig",
      "projectFolder": "rigs/heft-node-rig",
      "shouldPublish": false
    }
  ]
}
